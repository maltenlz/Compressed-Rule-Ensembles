library(cre)
data <- read.csv("magic.csv")
x = data[,-ncol(data)]
y = data[,ncol(data)]
system.time({cre_mod = cre(x = x,y = y, k = 2, task = "class")})
pROC::auc(y, predict(cre_mod, x, s = "lambda.1se"))
